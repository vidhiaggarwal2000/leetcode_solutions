class Solution {
public:
    int climbStairs(int n) {
       int x=1,y=2,v;
        if(n==1)
            return x;
        else if(n==2)
            return y;
        else
            for(int i=3;i<=n;i++){
                v=x+y;
                x=y;
                y=v;
            }
        return v;
    }
};
