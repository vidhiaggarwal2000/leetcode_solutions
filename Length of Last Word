class Solution {
public:
    int lengthOfLastWord(string s) {
        int count=0;
        int n=s.length()-1;
        int WordStartIndex=-1;
        while(n>=0&&s[n]==' '){
            n--;
        }
        WordStartIndex=n;
        while(n>=0&&s[n]!=' '){
            count++;
            n--;
        }
        return count;
    }
};
