class Solution {
public:
    int maximumProduct(vector<int>& nums) {
        if(nums.size()==3)
            return nums[0]*nums[1]*nums[2];
        sort(nums.begin(),nums.end());
        int n=nums.size()-1;
        if(nums[0]*nums[1]*nums[n]>nums[n]*nums[n-1]*nums[n-2])
            return nums[0]*nums[1]*nums[n];
        return nums[n]*nums[n-1]*nums[n-2];
    }
};
