/*if a number xor by itself then result is 0
  if a number is xor by zero then result is number itself*/
class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int res=nums[0];
        for(int i=1;i<nums.size();i++){
            res^=nums[i];
        }
        return res;
    }
};
